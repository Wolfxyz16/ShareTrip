<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane prefHeight="740.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eus.ehu.sharetrip.uicontrollers.videoController">
    <MediaView fx:id="video" fitHeight="500.0" fitWidth="700.0" layoutX="150.0" layoutY="78.0" />
   <Button fx:id="stopBtn" layoutX="346.0" layoutY="614.0" mnemonicParsing="false" onAction="#stop" text="stop">
      <styleClass>
         <String fx:value="btn" />
         <String fx:value="btn-lg" />
         <String fx:value="btn-primary" />
      </styleClass>
   </Button>
   <Button fx:id="playBtn" layoutX="594.0" layoutY="614.0" mnemonicParsing="false" onAction="#play" text="play">
      <styleClass>
         <String fx:value="btn" />
         <String fx:value="btn-lg" />
         <String fx:value="btn-primary" />
      </styleClass>
   </Button>


</AnchorPane>
